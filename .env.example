# Database Configuration
DB_TYPE=postgres                    # Change to 'sqlite' to use SQLite instead
DB_HOST=localhost                   # Hostname for database connection
DB_PORT=5432                        # Port for database connection
DB_USERNAME=postgres               # Username for database connection
DB_PASSWORD=postgres               # Password for database connection
DB_NAME=user_management_db         # Name of the database to use

# JWT Configuration
JWT_SECRET=your_super_secret_jwt_key_here  # Secret key for JWT tokens (generate a strong random key)

# Application Configuration
PORT=3000                           # Port on which the server will run